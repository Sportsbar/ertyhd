CREATE TABLE emplogtable (
    Id INT PRIMARY KEY IDENTITY(1,1),
    LogLevel NVARCHAR(50) NOT NULL,
    Source NVARCHAR(50) NOT NULL,
    Message NVARCHAR(MAX) NOT NULL,
    Exception NVARCHAR(MAX),
    CreatedOn DATETIME NOT NULL
);




