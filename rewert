 public IActionResult GetAssignee()
        {
            var employees = _context.xref_productdevelopment_team
                .Where(e => e.active == 1)
                .Select(e => new { e.firstName, e.lastName, e.employeeEmail, e.loginId })
                .ToList();

            var accounts = _context.assignee_accountid
                .ToList();

            return Json(new { employees, accounts });
        }
        
        
        
        
        function GetAssigneeDetails() {
            $.ajax({
                url: "/JiraTicketingTool/GetAssignee",
                type: "GET",
                dataType: "json",
                success: function (data) {
                    var options = "";
                    for (var i = 0; i < data.employees.length; i++) {
                        options += "<option value='" + data.employees[i].employeeEmail + "' data-account-id='" + getAccountId(data.accounts, data.employees[i].loginId) + "'>" + data.employees[i].firstName + " " + data.employees[i].lastName + " (" + data.employees[i].loginId + ")" + "</option>";
                    }
                    $("#Assignee").append(options);
                    // Initialize Select2 plugin
                    $('#Assignee').select2();
                }
            });
        }

        function getAccountId(accounts, loginId) {
            for (var i = 0; i < accounts.length; i++) {
                if (accounts[i].loginId === loginId) {
                    return accounts[i].accountId;
                }
            }
            return "";
        }
        
        
        
        
         var assigneeAccountId = $("#Assignee").find('option:selected').data('account-id');
         
         
         
         
         
         
         
           formData.append('accountId', assigneeAccountId);
